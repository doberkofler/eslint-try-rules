{
	"name": "eslint-try-rules",
	"version": "0.1.2",
	"type": "module",
	"license": "MIT",
	"author": "Dieter Oberkofler",
	"files": [
		"dist"
	],
	"keywords": [
		"eslint",
		"rules"
	],
	"bin": {
		"eslint-try-rules": "./dist/index.mjs"
	},
	"scripts": {
		"dev": "tsdown --watch",
		"build": "tsdown",
		"prepublishOnly": "npm run build",
		"lint": "eslint --no-cache . && tsc && prettier --check .",
		"test": "vitest run --coverage",
		"example": "tsx src/index.ts src --rules example/try-rules.jsonc",
		"ci": "npm run lint && npm run build && npm run test",
		"create-changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
		"pack-local": "rm -f *.tgz && npm run build && npm pack"
	},
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": "./dist/index.mjs",
		"./package.json": "./package.json"
	},
	"engines": {
		"node": ">=22.12.0"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/doberkofler/eslint-try-rules.git"
	},
	"homepage": "https://github.com/doberkofler/eslint-try-rules#readme",
	"devDependencies": {
		"@eslint/js": "^9.31.0",
		"@types/cli-progress": "^3.11.6",
		"@types/node": "^25.3.0",
		"@vitest/coverage-v8": "^4.0.18",
		"@vitest/eslint-plugin": "^1.6.9",
		"conventional-changelog": "^7.1.1",
		"conventional-changelog-angular": "^8.1.0",
		"conventional-changelog-cli": "^5.0.0",
		"eslint-plugin-jsdoc": "^62.6.1",
		"eslint-plugin-regexp": "^3.0.0",
		"eslint-plugin-unicorn": "^63.0.0",
		"globals": "^17.3.0",
		"prettier": "^3.8.1",
		"tsdown": "^0.20.3",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.0",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"ansis": "^4.2.0",
		"cli-progress": "^3.12.0",
		"commander": "^14.0.3",
		"eslint": "^9.31.0",
		"strip-json-comments": "^5.0.3",
		"tinyglobby": "^0.2.15",
		"zod": "^4.3.6"
	}
}
